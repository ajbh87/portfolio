{
    "plugins": [],
    "recurseDepth": 2,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": ["./" ],
        "exclude": ["./node-modules/", "./bundles/"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}