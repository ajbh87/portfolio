{
  "plugins": ["plugins/markdown"],  
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["./"],
    "exclude": [
      "./node_modules/",
      "./_site/",
      "./bundles/", 
      "./webpack.config.js"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "template": "./node_modules/docdash/",
    "recurse": true,
    "readme": "./README.md",
    "package": "./package.json"
  }
}
